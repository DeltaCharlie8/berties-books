<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopName%> Shop</h1>
    <h2>Here are the users currently registered:</h2>
    <table>
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%=user.first_name%></td>
          <td><%=user.last_name%></td>
          <td><%=user.email%></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
